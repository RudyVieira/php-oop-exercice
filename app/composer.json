{
    "name": "root/html",
    "type": "library",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "RudyVieira\\": "../src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}